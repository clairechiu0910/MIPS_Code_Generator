// A program to sum 1 to N
procedure SUM is
declare
	N: integer;
	S: integer;
	I: integer;
begin
	read N;
	if N < 0 then
		write -1;
		exit;
	else
		S := 0;
	end if;

	// Loop
	for I in 1..N loop
		S := S + I;
	end loop;
	write S;
	if A < 10 then
		A := 100;
	end if;
	for I in 1..N loop
		S := S + I;
	end loop;
end;

